VertexlitGeneric
{

	$vertexfog 1
	$treeSway			"2"	// 1 is for trees, 2 is for vines
	$treeSwayHeight			"-300"	// radius at which the effect is on fully (in world units)
	$treeSwayStartHeight		"0.1"	// portion of height at which the effect starts to fade in (0-1)
	$treeSwayRadius			"30"	// radius at which the effect is on fully (in world units)
	$treeSwayStartRadius		"0.125"	// portion of radius at which the effect starts to fade in (0-1)
	$treeSwaySpeed			"0.0125"	// how quickly the tree sways (as a multiplier of time)
	$treeSwayStrength		"1"	// how much the tree sways (as a multiplier of position)
	$treeSwayScrumbleSpeed		"5"	// how quickly the leaves move (as a multiplier of time)
	$treeSwayScrumbleStrength	"0.015"	// how much the leaves move( as a multiplier of position)
	$treeSwayScrumbleFrequency	"25"	// spatial frequency of sine wave applied to leaves.  Typically high-ish (10-25)
	$treeSwayFalloffExp		"1.4"	// falloff parameter for wave motion on branches/trunk, higher means core of tree is more stable
	$treeSwayScrumbleFalloffExp	"2"	// falloff parameter for scrumble motion on leaves, higher means the core of the tree is more stable
	$treeSwaySpeedHighWindMultiplier "25"  // multiplier of movement speed at higher wind
	$treeSwaySpeedLerpStart 	"2.0"	// wind speed at which high wind multiplier begins to have effect
	$treeSwaySpeedLerpEnd 		"6.0"	// wind speed at which high wind multiplier is fully on
	$LowQualityFlashlightShadows 1
}
