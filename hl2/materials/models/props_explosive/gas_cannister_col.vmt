"VertexLitGeneric"
{
	"$basetexture"	"Models/props_explosive/gas_cannister_col"	
	"$bumpmap"	      "Models/props_explosive/gas_cannister_normal"

	"$phong" "1" //this turns on the phong effect
	"$phongexponenttexture"  "Models/props_explosive/gas_cannister_specexponent"
